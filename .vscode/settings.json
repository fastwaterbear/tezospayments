{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "[typescript]": {
    "editor.tabSize": 2,
  },
  "[typescriptreact]": {
    "editor.tabSize": 2,
  },
  "[javascript]": {
    "editor.tabSize": 2
  },
  "[javascriptreact]": {
    "editor.tabSize": 2
  },
  "[scss]": {
    "editor.tabSize": 2
  },
  "[csharp]": {
    "editor.tabSize": 4
  },
  "[aspnetcorerazor]": {
    "editor.tabSize": 2
  },
  "files.insertFinalNewline": true,
  "files.eol": "\n",
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.quoteStyle": "single",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
  ],
  "cSpell.language": "en,ru",
  "cSpell.words": [
    "airgap",
    "antd",
    "bignumber",
    "browserslist",
    "ecadLabs",
    "echarts",
    "entrypoint",
    "entrypoints",
    "fastwaterbear",
    "florencenet",
    "granadanet",
    "hangzhounet",
    "micheline",
    "mutez",
    "popconfirm",
    "promisify",
    "quipuswap",
    "secp256k1",
    "serializable",
    "tezos",
    "tezospayments",
    "undeleting"
  ]
}
