{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "[typescript]": {
    "editor.tabSize": 2,
  },
  "[javascript]": {
    "editor.tabSize": 2
  },
  "[scss]": {
    "editor.tabSize": 2
  },
  "files.insertFinalNewline": true,
  "files.eol": "\n",
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.quoteStyle": "single",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
  ],
  "cSpell.language": "en,ru",
  "cSpell.words": [
    "antd",
    "promisify",
    "browserslist",
    "fastwaterbear",
    "tezos",
    "mutez",
    "entrypoint",
    "entrypoints"
  ]
}
