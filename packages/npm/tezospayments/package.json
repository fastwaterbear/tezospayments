{
  "name": "tezospayments",
  "version": "0.3.0",
  "description": "Package for payment generation in the Tezos Payments system.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.mjs",
  "browser": "dist/index.esm.js",
  "umd": "dist/index.umd.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs.js",
      "browser": "./dist/index.esm.js",
      "umd": "./dist/index.umd.js"
    },
    "./esnext": {
      "import": "./dist/esnext/index.esnext.mjs",
      "require": "./dist/esnext/index.esnext.cjs.js",
      "browser": "./dist/esnext/index.esnext.esm.js"
    },
    "./es5": {
      "import": "./dist/es5/index.es5.mjs",
      "require": "./dist/es5/index.es5.cjs.js",
      "browser": "./dist/es5/index.es5.esm.js",
      "umd": "./dist/es5/index.es5.umd.js"
    }
  },
  "dependencies": {
    "@tezospayments/common": "^0.4.0",
    "nanoid": "^3.1.25"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "babel-jest": "^27.0.6",
    "jest": "^27.0.6"
  },
  "scripts": {
    "build": "rollup --config",
    "test": "jest --coverage",
    "watch": "rollup --config --watch",
    "test-watch": "jest --watchAll",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx"
  },
  "typesVersions": {
    "*": {
      "es5": [
        "dist/types/index.d.ts"
      ],
      "esnext": [
        "dist/types/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "homepage": "https://tezospayments.com",
  "author": "Fast Water Bear",
  "keywords": [
    "tezos",
    "payments",
    "tezospayments"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fastwaterbear/tezospayments.git",
    "directory": "src/tezospayments"
  },
  "bugs": {
    "url": "https://github.com/fastwaterbear/tezospayments/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
